{
  "expo": {
    "icon": "./assets/logo.png",
    "scheme": "acme",
    "userInterfaceStyle": "light",
    "orientation": "default",
    "backgroundColor": "#FFFFFF",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splashscreen_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splashscreen_logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF"
        }
      ]
    ],
    "name": "Записная книжка",
    "slug": "zapisnaya-knizhka",
    "version": "1.0.0",
    "android": {
      "package": "com.zapisnayaknizhka.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": []
    },
    "ios": {
      "bundleIdentifier": "com.zapisnayaknizhka.app",
      "supportsTablet": true,
      "icon": "./assets/logo.png",
      "backgroundColor": "#FFFFFF"
    },
    "extra": {
      "themeColors": {
        "light": {
          "backgroundColor": "#FFFFFF",
          "splashBackgroundColor": "#FFFFFF"
        },
        "dark": {
          "backgroundColor": "#FFFFFF",
          "splashBackgroundColor": "#FFFFFF"
        }
      },
      "eas": {
        "projectId": "ec57d1b7-8ba8-440e-8839-2a5574f966f6"
      }
    },
    "owner": "vaces8v"
  }
}
